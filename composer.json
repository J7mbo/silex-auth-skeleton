            "homepage": "http://j7mbo.co.uk",
            "role": "Author, Developer"
        }
    ],
    "require": {
        "silex/silex": "1.1.*",
        "symfony/security": "2.3.*",
        "symfony/yaml": "2.4.*@dev",
        "twig/twig": ">=1.8,<2.0-dev",
        "symfony/twig-bridge": "2.3.*",
        "monolog/monolog": "1.6.*@dev",
        "rdlowrey/auryn": "dev-v1.0.0-dev",
